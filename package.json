{
  "name": "phenogrid",
  "version": "0.0.1",
  "description": "Monarch PhenoGrid widget",
  "scripts": {
    "build": "browserify -d entry.js -o bundle.js",
    "update-docs": "git checkout gh-pages && git pull && git merge master && gulp doc && git commit -a -m 'bump docs' && git push && git checkout master",
    "test": "./node_modules/.bin/gulp test"
  },
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monarch-initiative/phenogrid.git"
  },
  "keywords": [
    "monarch",
    "phenogrid",
    "widget",
    "visualization"
  ],
  "author": "University of Pittsburgh, Department of Biomedical Informatics",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monarch-initiative/phenogrid/issues"
  },
  "homepage": "https://github.com/monarch-initiative/phenogrid",
  "dependencies": {
    "browserify-shim": "^3.8.8",
    "d3": "^3.5.5",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "jshashtable": "^0.1.0",
    "requirejs": "^2.1.18",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-ui": "./node_modules/jquery-ui/jquery-ui.js",
    "d3": "./node_modules/d3/d3.min.js"
  },
  "style": [
    "./css/phenogrid.css",
    "./css/stickytooltip.css",
    "./node_modules/jquery-ui/themes/base/jquery-ui.css"
  ],
  "devDependencies": {
    "browserify": "^10.2.4",
    "chai": "^2.3.0",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-bump": "^0.3.0",
    "gulp-concat": "^2.5.2",
    "gulp-git": "^1.2.3",
    "gulp-jsdoc": "^0.1.4",
    "gulp-mocha": "^2.0.1",
    "gulp-pandoc": "^0.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.4.2",
    "gulp-uglify": "^1.2.0",
    "jsdoc": "^3.3.0",
    "jsdoc-baseline": "git://github.com/hegemonic/jsdoc-baseline.git#74d1dc8075"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "global:$"
    },
    "jquery-ui": {
      "depends": [
        "jquery"
      ]
    }
  },
  "main": "js/phenogrid.js"
}
