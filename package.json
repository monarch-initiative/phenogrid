    "buildandserve": "npm run devbuild && http-server -c-1",
    "pack": "npm run rmdsstore && npm run build && npm pack"
  },
  "dependencies": {
    "d3": "5.7.0",
    "font-awesome": "4.7.0",
    "gulp-uglify-es": "^3.0.0",
    "jquery": "^3.6.0",
    "jquery-ui": "1.13.0",
    "normalize.css": "8.0.1"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "file-saver": "^2.0.5",
    "gulp": "^4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.3",
    "gulp-eslint": "^6.0.0",
    "gulp-file-include": "^2.3.0",
    "gulp-mocha": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "^3.0.2",
    "http-server": "^13.0.0",
    "lodash": "^4.17.15",
    "vinyl-source-stream": "2.0.0"
