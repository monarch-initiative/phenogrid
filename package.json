  },
  "files": [
    "dist/",
    "js/",
    "README.md"
  ],
  "homepage": "https://github.com/monarch-initiative/phenogrid",
  "scripts": {
    "clean": "rm -rf dist/",
    "rmdsstore": "find . -name '.DS_Store' -print -delete",
    "lint": "eslint js/",
    "build": "gulp bundle",
